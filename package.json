{
  "name": "vite-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-on-change": "onchange 'src/**/*.{js,vue}' './*.{js,json}' -- npm run build",
    "cypress": "cypress open",
    "lint": "eslint ./src/**/*.{js,vue}",
    "test:e2e": "run-s build cypress",
    "watch:e2e": "run-p build-on-change serve cypress",
    "test:e2e-ci": "cypress run",
    "test": "jest src",
    "prepreview": "npm run build",
    "preview": "npm run serve",
    "serve": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.0-4",
    "tailwindcss": "^2.2.7",
    "vue": "^3.2.0",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/jest": "^26.0.24",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "^3.2.1",
    "@vue/test-utils": "^2.0.0-rc.12",
    "@vueuse/core": "^5.3.0",
    "autoprefixer": "^10.3.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-import-meta": "^2.0.0",
    "cypress": "^8.2.0",
    "cypress-watch-and-reload": "^1.4.2",
