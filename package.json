{
  "name": "vite-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-on-change": "onchange 'src/**/*.{js,vue}' './*.{js,json}' -- npm run build",
    "cypress": "cypress open",
    "lint": "eslint ./src/**/*.{js,vue}",
    "test:e2e": "run-s build cypress",
    "watch:e2e": "run-p build-on-change serve cypress",
    "test:e2e-ci": "cypress run",
    "test": "jest src",
    "prepreview": "npm run build",
    "preview": "npm run serve",
    "serve": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^3.0.0-3",
    "@tailwindcss/jit": "^0.1.3",
    "vue": "^3.0.5",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@types/jest": "^26.0.21",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.4",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-import-meta": "^1.0.1",
    "cypress": "^6.8.0",
    "cypress-watch-and-reload": "^1.3.6",
    "debounce": "^1.2.1",
    "delay": "^5.0.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.2.8",
    "tailwindcss": "^2.0.4",
    "ts-jest": "^26.5.4",
    "vite": "^2.1.0",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
